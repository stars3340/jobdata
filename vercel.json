{
  "version": 2,
  "name": "recruitment-dashboard",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "env": {
    "PYTHONPATH": "."
  },
  "regions": ["hkg1"]
} 